<% if(errors.length> 0) { %>
  <div class="alert alert-danger my-4" role="alert">
    <% errors.forEach(error=> { %>
      <div>
        <%=error%>
      </div>
      <% }) %>
  </div>
  <% } %>

    <% if(success.length> 0) { %>
      <div class="alert alert-success my-4" role="alert">
        <% success.forEach(success=> { %>
          <div>
            <%=success%>
          </div>
          <% }) %>
      </div>
      <% } %>